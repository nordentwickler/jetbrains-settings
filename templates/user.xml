<templateSet group="user">
  <template name="ready" value="$(function() {&#10;  $code$&#10;});" description="jQuery ready" toReformat="false" toShortenFQNames="true">
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="cl" value="console.log($message$);" description="Console log command" toReformat="true" toShortenFQNames="true">
    <variable name="message" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="if" value="if ($statement$) {&#10;    $code$&#10;}" description="If-block" toReformat="true" toShortenFQNames="true">
    <variable name="statement" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="ce" value="console.error($message$);" description="Console error command" toReformat="true" toShortenFQNames="true">
    <variable name="message" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="for" value="for ($i$ = $start$; $i$ &lt; $iteratable$.length; $i$++) {&#10;    $code$&#10;}" description="For loop" toReformat="true" toShortenFQNames="true">
    <variable name="i" expression="" defaultValue="i" alwaysStopAt="true" />
    <variable name="start" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="iteratable" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="af" value="function($arg$) {&#10;    $code$  &#10;}" description="Anonymous function" toReformat="true" toShortenFQNames="true">
    <variable name="arg" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="switch" value="{% switch $var$ %}&#10;    {% case &quot;$case$&quot; %}&#10;        $code$&#10;    {% default %}&#10;{% endswitch %}" description="Switch block" toReformat="true" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="block.type" alwaysStopAt="true" />
    <variable name="case" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="case" value="{% case &quot;$case$&quot; %}&#10;    $code$" description="Case tag of a switch block" toReformat="false" toShortenFQNames="true">
    <variable name="case" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="pubf" value="public function $NAME$($ARGS$)&#10;{&#10;    $CODE$&#10;}" description="PHP public method" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>