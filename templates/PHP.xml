<templateSet group="PHP">
  <template name="duskTest" value="/**&#10;     * A Dusk test.&#10;     * $DESCRIPTION$&#10;     * Arrange: $ARRANGE$&#10;     * Act:     $ACT$&#10;     * Assert:  $ASSERT$&#10;     *&#10;     * @return void&#10;     * @throws \Exception&#10;     * @throws \Throwable&#10;     */&#10;    public function test$NAME$()&#10;    {&#10;        $this-&gt;browse(function (Browser $browser) {&#10;            $browser&#10;                -&gt;visit('$URL$')&#10;                -&gt;$CODE$;&#10;        });&#10;    }" description="A dusk test case" toReformat="false" toShortenFQNames="true">
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARRANGE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ACT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ASSERT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="URL" expression="" defaultValue="/" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="testcase" value="&lt;?php&#10;    use Illuminate\Foundation\Testing\WithoutMiddleware;&#10;    use Illuminate\Foundation\Testing\DatabaseMigrations;&#10;    use Illuminate\Foundation\Testing\DatabaseTransactions;&#10;    &#10;    class $EXAMPLE$Test extends TestCase&#10;    {&#10;        $TESTS$&#10;    }" description="PHPUnit test case" toReformat="false" toShortenFQNames="true">
    <variable name="EXAMPLE" expression="" defaultValue="Example" alwaysStopAt="true" />
    <variable name="TESTS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="for" value="for ($i = $I$; $i &lt; $N$; $i++) {&#10;    $CODE$&#10;}" description="for($i=0; $i&lt;n; $i++)" toReformat="false" toShortenFQNames="true">
    <variable name="I" expression="" defaultValue="0" alwaysStopAt="true" />
    <variable name="N" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>